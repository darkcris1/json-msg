{
  "name": "json-msg",
  "version": "2.0.0",
  "description": "Fast, quite and beautiful messages for validating schema",
  "main": "dist/jm.cjs.js",
  "browser": "dist/jm.umd.min.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/",
    "dist/"
  ],
  "jest": {
    "testPathIgnorePatterns": []
  },
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.js",
    "clean": "rimraf dist coverage",
    "test": "jest --maxWorkers=1",
    "test:dev": "jest --watchAll --maxWorkers=1",
    "pub": "npm version patch && npm run build && npm publish",
    "pub:minor": "npm version minor && npm run build && npm publish"
  },
  "keywords": [
    "validator",
    "json",
    "messages",
    "schema",
    "json validator",
    "joi"
  ],
  "bugs": {
    "url": "https://github.com/darkcris1/json-msg/issues"
  },
  "repository": {
    "url": "https://github.com/darkcris1/json-msg.git",
    "type": "git"
  },
  "author": "Cris Fandi√±o <sircnujnuj@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "~7.12.1",
    "@babel/preset-env": "~7.12.1",
    "@babel/register": "~7.12.1",
    "coveralls": "~3.1.0",
    "cross-env": "~7.0.2",
    "eslint": "~7.12.1",
    "jest": "~26.6.3",
    "npm-run-all": "~4.1.5",
    "rimraf": "~3.0.2",
    "rollup": "~2.33.1",
    "rollup-plugin-babel": "~4.4.0",
    "rollup-plugin-commonjs": "~10.1.0",
    "rollup-plugin-node-resolve": "~5.2.0",
    "rollup-plugin-terser": "~7.0.2"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead"
}
